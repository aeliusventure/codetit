			<Templates>
				<ItemTemplate id="row" name="rowTemplate">
					<View id="imageContainer">
						<ImageView id="image" bindId="image" />
					</View>
					<View id="infoContainer">
						<Label id="updated_at" bindId="updated_at" class="itemLabel" />
						<Label id="place" bindId="place" class="itemLabel" />
					</View>
				</ItemTemplate>
			</Templates>
			<ListSection></ListSection>
		</ListView>
	</Window>
</Alloy>
